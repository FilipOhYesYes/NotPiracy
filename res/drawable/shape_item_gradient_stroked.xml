<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@id/shape_bg">
        <shape android:shape="rectangle">
            <corners android:radius="12.0dip" />
            <gradient android:startColor="#ffdeebff" android:endColor="#5cdeebff" android:angle="0.0" />
        </shape>
    </item>
    <item android:id="@id/shape_stroke">
        <shape android:shape="rectangle">
            <corners android:radius="12.0dip" />
            <stroke android:width="1.0dip" android:color="#ffd9e7fd" />
        </shape>
    </item>
</layer-list>